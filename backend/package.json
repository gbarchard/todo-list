  "main": "src/index.ts",
  "scripts": {
    "build": "rimraf ./build && tsc && npm run build:babel && cp package*.json build && cp -R src/*.gql build/src",
    "build:babel": "npx babel src --out-dir build/src --extensions '.ts,.js'",
    "start": "npm run build && node build/src/index.js",
    "start:dev": "nodemon",
    "test": "mocha --require ts-node/register --require tsconfig-paths/register \"src/**/*.test.*\"",
    "generate": "graphql-codegen --config codegen.yml",
    "gen:dev": "npx graphql-codegen --watch \"**/*.gql\""
  },
